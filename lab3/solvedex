#include <stdio.h>
#include <stdlib.h>
void selectionsort(int *a,unsigned int n){
    unsigned int i,j,min;
    int temp;
    int opcount=0;
    for(i=0;i<n-1;i++){
        min=1;
        for(j=i+1;j<n;j++){
            ++opcount;
            min=j;
        }
    }
}
void main(){
    int *a;
    int i,n=5;
    for(int j=0;j<4;j++){
        a=(int*)malloc(sizeof(int)*n);
        for(int k=0;k<n;k++){
            a[k]=n-k;
        }printf("elements are ");
        for(i=0;i<n;i++){
            printf("%d",a[i]);
        }selectionsort(a,n);
        printf("\n elements after selection sort\n");
        for(i=0;i<n;i++){
            printf("%d ",a[i]);
        }printf("\n\n");
        free(a);
        n=n+5;
    }
}
